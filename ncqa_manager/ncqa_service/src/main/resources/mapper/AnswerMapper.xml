<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.neusoft.xuxiao.mapper.AnswerMapper">
    <resultMap id="BaseResultMap" type="cn.neusoft.xuxiao.entity.Answer">
        <result column="id" property="id"/>
        <result column="question_id" property="questionId"/>
        <result column="answer_index" property="answerIndex"/>
        <result column="answer_content" property="answerContent"/>
        <result column="answer_type" property="answerType"/>
        <result column="is_right" property="isRight"/>
    </resultMap>
    <sql id="Base_Column_List">
        id,
        question_id AS questionId,
        answer_index AS answerIndex,
        answer_content AS answerContent,
        answer_type AS answerType,
        is_right AS isRight
    </sql>
</mapper>